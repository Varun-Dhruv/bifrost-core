{
    "data": {
        "sources": [
            {
                "type": "stream",
                "method": "kafka",
                "path": {
                    "url": "pkc-p11xm.us-east-1.aws.confluent.cloud:9092",
                    "topic": "car-data"
                },
                "transformations": [
                    {
                        "remove_null": {
                            "fields": [
                                "Horsepower",
                                "TemperatureEndurance"
                            ]
                        }
                    }
                ],
                "sink":{
                    "type":"batch",
                    "method":"file",
                    "path":{
                        "relative":"output.json"
                    }
                }
            },
            {
                "type": "batch",
                "method": "mongodb",
                "path": {
                    "url": "mongodb://0.tcp.in.ngrok.io:14416/",
                    "database": "test",
                    "collection": "cars"
                },
                "transformations": [
                    {
                        "remove_null": {
                            "fields": [
                                "tag",
                                "offer"
                            ]
                        }
                    }
                ],
                 "sink":{
                    "type":"batch",
                    "method":"file",
                    "path":{
                        "relative":"output.json"
                    }
                }
            }
        ]
    }
}
